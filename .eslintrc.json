{
	"plugins": ["prettier"],
	"rules": {
    "prettier/prettier": "error"
  	},
    "extends": [
        "eslint:recommended",
		"strongloop",
		"prettier",
		"plugin:prettier/recommended"
    ],
    "parserOptions": {
        // Only ESLint 6.2.0 and later support ES2020.
        "ecmaVersion": 2020
    }
}